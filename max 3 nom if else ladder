#include <stdio.h>
int main()
{
    int a,b,c,max;
    printf("Enter the values of of a,b,c: \n");
    scanf("%d%d%d",&a,&b,&c);
    
    if ((a>b)&&(a>c))
    {
        printf("%d is max",a);
    }
    
    else if ((b>a)&&(b>c))
    {
        printf("%d is max",b);
    }
    
    else
    {
        printf("%d is max",c);
    }
}
