#include <stdio.h>
int main()
{
    printf("Enter any number to check if its prime or not: ");
    int i,n,flag=0;
    scanf("%d",&n);
    
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
        {
            printf("Entered number: %d is NOT a Prime no",n);
            flag=1;
            break;
        }
    }
    if (flag==0)
        printf("Entered number: %d is a Prime no",n);
}
